{
    "spec_dir": "tests",
    "spec_files": [
      "**/*[sS]pec.?(m)js"
    ],
    "helpers": [
      "jasmine/reporter.js",
      "jasmine/timeout.js"
    ],
    "env": {
      "stopSpecOnExpectationFailure": false,
      "random": true,
      "failFast": true,
      "stopOnSpecFailure": true,
      "hideDisabled": true
    }
  }